{
  "name": "@andriikot/codewars-bot",
  "version": "0.1.1-rc.5",
  "description": "Automates Codewars progress: sync solved katas into git repository",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "bin": {
    "codewars-bot": "./bin/cli.js"
  },
  "main": "./backend/index.js",
  "exports": {
    ".": "./backend/index.js"
  },
  "files": [
    "bin",
    "backend",
    "shared",
    "config.js"
  ],
  "engines": {
    "node": ">=18"
  },
  "imports": {
    "#config": "./config.js",
    "#templates": "./backend/templates/index.js",
    "#commands": "./backend/commands/index.js",
    "#cli": "./backend/cli/index.js",
    "#bootstrap": "./backend/bootstrap/index.js",
    "#db": "./backend/infrastructure/db/index.js",
    "#cache": "./backend/infrastructure/cache/index.js",
    "#storage": "./backend/services/storage/index.js",
    "#schemas": "./shared/schemas/index.js",
    "#schemas-validation-dynamic": "./shared/schemas/validation/schemas/dynamic/index.js",
    "#utils": "./shared/utils/index.js",
    "#contracts": "./shared/contracts/index.js",
    "#api": "./backend/infrastructure/index.js",
    "#services": "./backend/services/index.js",
    "#hash": "./backend/services/hash/index.js",
    "#helpers": "./backend/helpers/index.js"
  },
  "scripts": {
    "start": "node backend/index.js",
    "test": "node --test"
  }
}
